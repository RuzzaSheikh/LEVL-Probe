FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        GaussSeidel;
        tolerance       1e-7;
        relTol          0.001;
    }

    Phi
    {
        $p;
    }

    Urel
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        tolerance       1e-8;
        relTol          0.001;
        nSweeps         1;
    }

    k
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        tolerance       1e-8;
        relTol          0.001;
        nSweeps         1;
    }

    "(omega|nuTilda|f|v2)"
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        tolerance       1e-8;
        relTol          0.001;
        nSweeps         1;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
    consistent yes;
//pRefCell    0;
//pRefValue    0;
    residualControl
    {
        p               1e-4;
        Urel               1e-4;
        "(k|epsilon|omega|f|v2)" 1e-4;
    }

}

potentialFlow
{
    nNonOrthogonalCorrectors 10;
}

relaxationFactors
{
    equations
    {
        Urel		0.9;
        k               0.5;
        omega           0.5;
    }
}

cache
{
    grad(Urel);
}

